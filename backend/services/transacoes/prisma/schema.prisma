datasource db {
  provider = "sqlite"
  url      = "file:./transacoes.db"
}

generator client {
  provider = "prisma-client-js"
}

model Transacao {
  id                 Int      @id @default(autoincrement())
  idConta            Int
  tipo               String
  valor              Float
  dataHora           DateTime @default(now())
  saldoAntes         Float
  saldoDepois        Float
  idClienteOperador  Int?
}
